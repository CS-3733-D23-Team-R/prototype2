   <top>
      <AnchorPane BorderPane.alignment="CENTER">
         <children>
            <Rectangle arcHeight="5.0" arcWidth="5.0" fill="#003da6" height="80.0" stroke="#003da6" strokeType="INSIDE" width="1280.0" />
            <Text fill="WHITE" layoutX="454.0" layoutY="52.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Import New CSV File">
               <font>
                  <Font name="Times New Roman" size="36.0" />
               </font>
            </Text>
         </children>
      </AnchorPane>
   </top>
   <center>
      <AnchorPane>
         <children>
            <MFXButton fx:id="fileButton" layoutX="390.0" layoutY="295.0" text="Choose File" AnchorPane.topAnchor="295.0">
